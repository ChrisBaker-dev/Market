<div class='form'>
    <h2>Create Listing</h2>
    <%= form_with model: @listing, local: true do |f| %>
        <p>
            <%= f.label :title %>
            <%= f.text_field :title %>
        </p>
        <p>
            <%= f.label :description %>
            <%= f.text_area :description %>
        </p>
        <p>
            <%= f.label :price %>
            <%= f.text_field :price %>
        </p>
        <p>
            <%= f.label :qty %>
            <%= f.text_area :qty %>
        </p>
        <p>
            <%= f.label :image%>
            <%= f.file_field :image%>
        </p>
        <p>
            <%= f.submit %>
        </p>
    <% end %>
</div>